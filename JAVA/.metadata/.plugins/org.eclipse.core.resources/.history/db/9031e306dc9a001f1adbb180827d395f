package java_basics;

import java.util.Arrays;

public class SingleDimArray {

	public static void arrayInfo() {
		String[] tech = { "JAVA", "PYTHON", "c#", "JAVASCRIPT", "GO", "R", ".NET" };
		System.out.println(Arrays.toString(tech));
		System.out.println("Length of the array is " + tech.length);
		System.out.println("Printing the elements ");
		for (String proLanguage : tech) {
			System.out.println(proLanguage);
		}

	}

}
